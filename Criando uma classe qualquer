// Criação da Classe
public class Funcionario
{
    public String nome;
    public String CPF;
    public String email;

    public void imprimir ()
    {
        System.out.println(nome);
        System.out.println(CPF);
        System.out.println(email);
    }
}

// Criação de objeto func1 no Main

public class Main
{
    public static void main(String[] args)
    {
        Funcionario func1;
        func1 = new Funcionario();
        func1.nome = "Giovanni";
        func1.email = "gio@qqq.com";
        func1.CPF = "123456";
       
        func1.imprimir();
       
       
    }
}
